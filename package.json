{
  "name": "@prostojs/dye",
  "version": "0.0.1-alpha.0",
  "description": "Easy and light console styling tool",
  "main": "index.js",
  "module": "dist/dye.esm-bundler.js",
  "unpkg": "dist/dye.global.js",
  "jsdelivr": "dist/dye.global.js",
  "types": "dist/dye.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "scripts": {
    "build": "node ./scripts/build",
    "release": "node ./scripts/release",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --coverage",
    "lint": "eslint --ext .ts src/**.ts --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prostojs/dye.git"
  },
  "keywords": [
    "colors",
    "dye",
    "palette",
    "console",
    "prostojs",
    "style"
  ],
  "buildOptions": {
    "name": "ProstoDye",
    "formats": [
      "esm-bundler",
      "esm-browser",
      "cjs",
      "global"
    ]
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.15.1",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "brotli": "^1.3.2",
    "conventional-changelog": "^3.1.24",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.2.5",
    "rollup": "^2.58.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "zlib": "^1.0.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prostojs/dye/issues"
  },
  "homepage": "https://github.com/prostojs/dye#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.6",
    "chalk": "^4.1.2",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "minimist": "^1.2.5",
    "semver": "^7.3.5"
  }
}
