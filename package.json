{
  "name": "@prostojs/dye",
  "version": "0.3.0",
  "description": "Easy and light console styling tool",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/plugins/vite.d.mts",
      "import": "./dist/plugins/vite.mjs"
    },
    "./rolldown": {
      "types": "./dist/plugins/rolldown.d.mts",
      "import": "./dist/plugins/rolldown.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rolldown -c rolldown.config.mjs",
    "release": "node ./scripts/release",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "lint": "eslint --ext .ts src/**.ts --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prostojs/dye.git"
  },
  "keywords": [
    "colors",
    "dye",
    "palette",
    "console",
    "prostojs",
    "style"
  ],
  "buildOptions": {
    "name": "ProstoDye",
    "formats": [
      "esm-bundler",
      "esm-browser",
      "cjs",
      "global"
    ]
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prostojs/dye/issues"
  },
  "homepage": "https://github.com/prostojs/dye#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "@vitest/coverage-v8": "^2.1.8",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "eslint": "8.55.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-array-func": "4.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "2.5.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-yml": "1.10.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "minimist": "^1.2.5",
    "prettier": "3.1.1",
    "rolldown": "1.0.0-beta.1",
    "semver": "^7.3.5",
    "typescript": "^5.7.3",
    "unplugin-isolated-decl": "^0.10.4",
    "vite": "^6.0.7",
    "vitest": "^2.1.8",
    "yorkie": "^2.0.0",
    "zlib": "^1.0.5"
  }
}
